using System;

class Fibonacci
{
    static void Main(string[] args)
    {
        
        Console.Write("Enter the number of terms: ");
        int num = int.Parse(Console.ReadLine());

       
        int a = 0, b = 1, c;

        
        for (int i = 0; i < num; i++)
        {
                
            c = a + b;
            a = b;
            b = c;
        }
        Console.WriteLine("Fibonacci number is: " + b);
    }
}
